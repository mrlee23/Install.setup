# -*- mode: org -*-

#+MACRO: REDIRECT @@html:<script type="javascript">location.href = "$1"</script>@@
#+MACRO: INCLUDE_DOCS (eval (mapconcat (lambda (dir-name) (let ((name (substring dir-name 0 -4))) (format "** %s\n#+INCLUDE: %s :lines \"6-\"" name dir-name))) (seq-filter (lambda (dir) (and (string-match "^\\([a-zA-Z0-9-_]+\\)\\.org$" dir) (not (equal dir "README.org")))) (directory-files "./")) "\n"))
